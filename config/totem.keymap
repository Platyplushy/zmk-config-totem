            //                                     ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛   ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛

            bindings = <
                &kp Q           &kp W  &kp E   &kp R      &kp T        &kp Y    &kp U  &kp I      &kp O       &kp P
                &kp A           &kp S  &kp D   &kp F      &kp G        &kp H    &kp J  &kp K      &kp L       &kp LEFT_BRACKET
&kp LEFT_SHIFT  &mt LEFT_ALT Z  &kp X  &kp C   &kp V      &kp B        &kp N    &kp M  &kp COMMA  &kp PERIOD  &mt RIGHT_ALT SEMI  &kp SQT
                                       &tog 2  &kp LCTRL  &kp SPACE    &kp RET  &mo 1  &kp BSPC
